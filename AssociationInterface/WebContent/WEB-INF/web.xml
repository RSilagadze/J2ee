<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>AssociationInterface</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <context-param>
    <param-name>command_tag</param-name>
    <param-value>command</param-value>
  </context-param>
  <context-param>
    <param-name>action_tag</param-name>
    <param-value>action</param-value>
  </context-param>
  <context-param>
    <param-name>exception_tag</param-name>
    <param-value>exception</param-value>
  </context-param>
  <context-param>
    <param-name>user_tag</param-name>
    <param-value>user</param-value>
  </context-param>
  <context-param>
    <param-name>userId_tag</param-name>
    <param-value>userID</param-value>
  </context-param>
  <context-param>
    <param-name>sessionId_tag</param-name>
    <param-value>sessionId</param-value>
  </context-param>
  <context-param>
    <param-name>imageHost</param-name>
    <param-value>http://localhost/images/</param-value>
  </context-param>
  <servlet>
    <servlet-name>Faces Servlet</servlet-name>
    <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>Faces Servlet</servlet-name>
    <url-pattern>/faces/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>MainServlet</servlet-name>
    <servlet-class>com.itparis.b3.associations.servlets.MainServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MainServlet</servlet-name>
    <url-pattern>/main</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ConnectionServlet</servlet-name>
    <servlet-class>com.itparis.b3.associations.servlets.ConnectionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ConnectionServlet</servlet-name>
    <url-pattern>/connection</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UserServlet</servlet-name>
    <servlet-class>com.itparis.b3.associations.servlets.UserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/user</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AssociationServlet</servlet-name>
    <servlet-class>com.itparis.b3.associations.servlets.AssociationServlet</servlet-class>
    <init-param>
      <param-name>associationId_tag</param-name>
      <param-value>associationId</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>AssociationServlet</servlet-name>
    <url-pattern>/association</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LoginPage</servlet-name>
    <jsp-file>/JSP_content/Login.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminProfile</servlet-name>
    <url-pattern>/adminProfile</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AdminProfile</servlet-name>
    <jsp-file>/JSP_content/AdminProfile.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginPage</servlet-name>
    <url-pattern>/loginPage</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ManageAssociation</servlet-name>
    <jsp-file>/JSP_content/ManageAssociation.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>ManageAssociation</servlet-name>
    <url-pattern>/ManageAssociationPage</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UserProfilePage</servlet-name>
    <jsp-file>/JSP_content/UserProfile.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserProfilePage</servlet-name>
    <url-pattern>/userProfile</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AssociationPage</servlet-name>
    <jsp-file>/JSP_content/AssociationPage.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>AssociationPage</servlet-name>
    <url-pattern>/associationPage</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UserModPage</servlet-name>
    <jsp-file>/JSP_content/UserModPage.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserModPage</servlet-name>
    <url-pattern>/userModPage</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ListeAssocPage</servlet-name>
    <jsp-file>/JSP_content/ListAssociationsPage.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>ListeAssocPage</servlet-name>
    <url-pattern>/listeAssociationsPage</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ErrorPage</servlet-name>
    <jsp-file>/Errorpage.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>ErrorPage</servlet-name>
    <url-pattern>/error</url-pattern>
  </servlet-mapping>
</web-app>